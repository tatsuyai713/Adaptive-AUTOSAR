# AUTOSAR watchdog supervisor environment

AUTOSAR_AP_PREFIX="__AUTOSAR_AP_PREFIX__"
AUTOSAR_WATCHDOG_BIN="__AUTOSAR_AP_PREFIX__/bin/autosar_watchdog_supervisor"
AUTOSAR_WATCHDOG_DEVICE="/dev/watchdog"
AUTOSAR_WATCHDOG_INTERVAL_MS="1000"
AUTOSAR_WATCHDOG_ALLOW_SOFT_MODE="true"
AUTOSAR_WATCHDOG_HEARTBEAT_FILE="/run/autosar/watchdog.status"

