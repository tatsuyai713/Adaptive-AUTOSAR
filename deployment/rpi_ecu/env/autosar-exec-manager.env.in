# AUTOSAR execution manager bridge environment

AUTOSAR_AP_PREFIX="__AUTOSAR_AP_PREFIX__"
AUTOSAR_USER_APPS_BUILD_DIR="__USER_APPS_BUILD_DIR__"

# User-owned startup script (edit this file to launch your own applications)
AUTOSAR_BRINGUP_SCRIPT="/etc/autosar/bringup.sh"

# Optional SOME/IP configuration file path
# VSOMEIP_CONFIGURATION="/opt/autosar_ap/configuration/vsomeip-rpi.json"
# Optional fixed SOME/IP routing manager application name
# AUTOSAR_VSOMEIP_ROUTING_APP="autosar_vsomeip_routing_manager"

# User-app registration/health monitoring runtime files (consumed by bringup.sh
# and autosar-user-app-monitor.service).
AUTOSAR_RUN_DIR="/run/autosar"
AUTOSAR_USER_APP_REGISTRY_FILE="/run/autosar/user_apps_registry.csv"
AUTOSAR_USER_APP_STATUS_DIR="/run/autosar/user_apps"
AUTOSAR_PHM_HEALTH_DIR="/run/autosar/phm/health"
# Default heartbeat timeout in ms used by bringup helper (0 disables heartbeat check).
AUTOSAR_USER_APP_DEFAULT_HEARTBEAT_TIMEOUT_MS="0"
# Default restart policy used by bringup helper API.
AUTOSAR_USER_APP_DEFAULT_RESTART_LIMIT="5"
AUTOSAR_USER_APP_DEFAULT_RESTART_WINDOW_MS="60000"
# Base shortname-path prefix used by bringup helper for instance specifiers.
AUTOSAR_USER_APP_INSTANCE_PREFIX="AdaptiveAutosar/UserApps"
