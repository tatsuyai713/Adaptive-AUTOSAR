# AUTOSAR Network Management daemon environment
AUTOSAR_AP_PREFIX="__AUTOSAR_AP_PREFIX__"
AUTOSAR_NM_BIN="__AUTOSAR_AP_PREFIX__/bin/autosar_network_manager"
AUTOSAR_NM_CHANNELS="can0"
AUTOSAR_NM_PERIOD_MS="100"
AUTOSAR_NM_TIMEOUT_MS="5000"
AUTOSAR_NM_REPEAT_MSG_TIME_MS="1500"
AUTOSAR_NM_WAIT_BUS_SLEEP_MS="2000"
