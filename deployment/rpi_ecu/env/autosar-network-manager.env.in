AUTOSAR_NM_PARTIAL_NETWORKING="false"
AUTOSAR_NM_STATUS_FILE="/run/autosar/network_manager.status"
AUTOSAR_NM_TRIGGER_DIR="/run/autosar/nm_triggers"
