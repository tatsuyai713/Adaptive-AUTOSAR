# AUTOSAR UCM (Update & Configuration Management) daemon environment
AUTOSAR_AP_PREFIX="__AUTOSAR_AP_PREFIX__"
AUTOSAR_UCM_BIN="__AUTOSAR_AP_PREFIX__/bin/autosar_ucm_daemon"
AUTOSAR_UCM_STAGING_DIR="/var/lib/autosar/ucm/staging"
AUTOSAR_UCM_PROCESSED_DIR="/var/lib/autosar/ucm/processed"
AUTOSAR_UCM_STATUS_FILE="/run/autosar/ucm_daemon.status"
AUTOSAR_UCM_SCAN_PERIOD_MS="2000"
AUTOSAR_UCM_AUTO_ACTIVATE="true"
